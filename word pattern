#include<stdio.h>
#include<conio.h>
void main()
{
    int i,j;
    for(i=0;i<10;i++)
    {
        for(j=0;j<10;j++)
        {
            if(j==0 || j==9)
            {
                printf("*");
            }
            else if(i+j==9 && i>=5)
            {
                printf("*");
            }
            else if(i==j && i>=5)
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }
}
