#include<stdio.h>
#include<string.h>
void main()
{
    int n;
    scanf("%d",&n);
    char names[n][10];
    char temp[10];
    int i,j;
    for(i=0;i<n;i++)
    {
        scanf("%s",names[i]);
    }
    for(i=0;i<n;i++)
    {
        for(j=i;j<n;j++)
        {
            if(strcmp(names[i],names[j])>0)
            {
                strcpy(temp,names[i]);
                strcpy(names[i],names[j]);
                strcpy(names[j],temp);
            }
        }
    }
    for(i=0;i<n;i++)
    {
        printf("%d.%s\n",(i+1),names[i]);
    }
}
