#include<stdio.h>
#include<conio.h>
#include<string.h>
void main()
{
    char para[100];
    printf("enter any sentence");
    gets(para);
    int c=0;
    for(int i=0;i<strlen(para);i++)
    {
        if(para[i]==' ' && para[i+1]!=' ')
        {
            c=c+1;
        }
         
    }        
    printf("the num of words %d",c+1);
}
